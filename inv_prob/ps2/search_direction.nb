Notebook[{
Cell["\<\
H is the Hessian Matrix of f(x,y):\
\>", "Text",
 CellChangeTimes->{{3.6018322592592173`*^9, 3.601832293946904*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"H", "=", 
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      RowBox[{
       RowBox[{"Cos", "[", "x", "]"}], "*", 
       RowBox[{"Cos", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}], 
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Sin", "[", "x", "]"}]}], "/", "10"}], "*", 
       RowBox[{"Sin", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}]},
     {
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Sin", "[", "x", "]"}]}], "/", "10"}], "*", 
       RowBox[{"Sin", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}], 
      RowBox[{
       RowBox[{
        RowBox[{"Cos", "[", "x", "]"}], "/", "100"}], "*", 
       RowBox[{"Cos", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}]}
    }], "\[NoBreak]", ")"}]}]], "Input",
 CellChangeTimes->{{3.601831815558832*^9, 3.601831929405505*^9}, 
   3.601832261496909*^9}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"Cos", "[", "x", "]"}], " ", 
      RowBox[{"Cos", "[", 
       FractionBox["y", "10"], "]"}]}], ",", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "10"]}], " ", 
      RowBox[{"Sin", "[", "x", "]"}], " ", 
      RowBox[{"Sin", "[", 
       FractionBox["y", "10"], "]"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "10"]}], " ", 
      RowBox[{"Sin", "[", "x", "]"}], " ", 
      RowBox[{"Sin", "[", 
       FractionBox["y", "10"], "]"}]}], ",", 
     RowBox[{
      FractionBox["1", "100"], " ", 
      RowBox[{"Cos", "[", "x", "]"}], " ", 
      RowBox[{"Cos", "[", 
       FractionBox["y", "10"], "]"}]}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.601831859676982*^9, 3.601831929756218*^9}, 
   3.601832267717514*^9}]
}, Open  ]],

Cell["\<\
// g is the (negative!) gradient of f(x,y):\
\>", "Text",
 CellChangeTimes->{{3.601832213818815*^9, 3.601832250938468*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"g", " ", "=", " ", 
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Sin", "[", "x", "]"}]}], "*", 
       RowBox[{"Sin", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}]},
     {
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Cos", "[", "x", "]"}]}], "/", "10"}], "*", 
       RowBox[{"Sin", "[", 
        RowBox[{"y", "/", "10"}], "]"}]}]}
    }], "\[NoBreak]", ")"}]}]], "Input",
 CellChangeTimes->{{3.601831941534842*^9, 3.6018319848546658`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Sin", "[", "x", "]"}]}], " ", 
     RowBox[{"Sin", "[", 
      FractionBox["y", "10"], "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", "10"]}], " ", 
     RowBox[{"Cos", "[", "x", "]"}], " ", 
     RowBox[{"Sin", "[", 
      FractionBox["y", "10"], "]"}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.6018319659809217`*^9, 3.601831985238996*^9}, 
   3.6018322700357122`*^9}]
}, Open  ]],

Cell["\<\
Here, we take the product of the inverse of H with g to find p_k, the search \
direction:\
\>", "Text",
 CellChangeTimes->{{3.601832300618976*^9, 3.601832316834494*^9}, {
  3.601832360434771*^9, 3.601832389395092*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"MatrixForm", "[", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{
    RowBox[{"Inverse", "[", "H", "]"}], ".", "g"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.601831994110004*^9, 3.601832064876008*^9}, {
   3.601832106925555*^9, 3.601832112475792*^9}, {3.60183216364425*^9, 
   3.601832196852584*^9}, 3.601832264644665*^9}],

Cell[BoxData[
 TagBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      FractionBox[
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"2", " ", "x"}], "]"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          RowBox[{"Cos", "[", 
           FractionBox["y", "5"], "]"}], "-", 
          RowBox[{"Sin", "[", 
           FractionBox["y", "5"], "]"}]}], ")"}]}], 
       RowBox[{"2", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "x"}], "]"}], "+", 
          RowBox[{"Cos", "[", 
           FractionBox["y", "5"], "]"}]}], ")"}]}]]},
     {
      RowBox[{"-", 
       FractionBox[
        RowBox[{"20", " ", 
         RowBox[{"Sin", "[", 
          FractionBox["y", "10"], "]"}], " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            SuperscriptBox[
             RowBox[{"Cos", "[", "x", "]"}], "2"], " ", 
            RowBox[{"Cos", "[", 
             FractionBox["y", "10"], "]"}]}], "+", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Sin", "[", "x", "]"}], "2"], " ", 
            RowBox[{"Sin", "[", 
             FractionBox["y", "10"], "]"}]}]}], ")"}]}], 
        RowBox[{
         RowBox[{"Cos", "[", 
          RowBox[{"2", " ", "x"}], "]"}], "+", 
         RowBox[{"Cos", "[", 
          FractionBox["y", "5"], "]"}]}]]}]}
    },
    GridBoxAlignment->{
     "Columns" -> {{Center}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
      "RowsIndexed" -> {}},
